    "from sklearn.preprocessing import RobustScaler\n",
    "\n",
    "# Preprocessing - All categorical with Onehot + RobustScaler for annual_income\n",
    "# grade mapping\n",
    "grade_mapping = {\n",
    "    'A1': 29, 'A2': 28, 'A3': 27, 'A4': 26, 'A5': 25,\n",
    "    'B1': 24, 'B2': 23, 'B3': 22, 'B4': 21, 'B5': 20,\n",
    "    'C1': 19, 'C2': 18, 'C3': 17, 'C4': 16, 'C5': 15,\n",
    "    'D1': 14, 'D2': 13, 'D3': 12, 'D4': 11, 'D5': 10,\n",
    "    'E1': 9, 'E2': 8, 'E3': 7, 'E4': 6, 'E5': 5,\n",
    "    'F1': 4, 'F2': 3, 'F3': 2, 'F4': 1, 'F5': 0\n",
    "}\n",
    "\n",
    "# map grade_subgrade\n",
    "def encode_grade(X):\n",
    "    X_copy = X.copy()\n",
    "    X_copy['grade_subgrade'] = X_copy['grade_subgrade'].map(grade_mapping)\n",
    "    return X_copy\n",
    "\n",
    "# categorical columns to encode\n",
    "categorical_features = ['education_level', 'gender', 'marital_status',\n",
    "                        'employment_status', 'loan_purpose']\n",
    "\n",
    "# numerical column to scale\n",
    "numerical_features = ['annual_income']\n",
    "\n",
    "preprocessor = ColumnTransformer(\n",
    "    transformers=[\n",
    "        ('cat', OneHotEncoder(drop='first', sparse_output=False), categorical_features),\n",
    "        ('num', RobustScaler(), numerical_features)\n",
    "    ],\n",
    "    remainder='passthrough'  # Keep other columns unchanged\n",
    ")\n",
    "\n",
    "# Create pipeline with grade encoding and preprocessor\n",
    "pipeline_robust = make_pipeline(\n",
    "    FunctionTransformer(encode_grade, validate=False),\n",
    "    preprocessor\n",
    ")\n",
    "\n",
    "# Fit and transform X_train\n",
    "X_train_preprocessed = pipeline_robust.fit_transform(X_train)\n",
    "\n",
    "# Get feature names and convert to DataFrame\n",
    "feature_names = pipeline_robust.named_steps['columntransformer'].get_feature_names_out()\n",
    "X_train_preprocessed = pd.DataFrame(X_train_preprocessed, columns=feature_names, index=X_train.index)\n",
    "\n",
    "# View the preprocessed data\n",
    "X_train_preprocessed.head()"
   ]
  },
