    "# preprocessing - education_level as ordinal\n",
    "# grade mapping\n",
    "grade_mapping = {\n",
    "    'A1': 29, 'A2': 28, 'A3': 27, 'A4': 26, 'A5': 25,\n",
    "    'B1': 24, 'B2': 23, 'B3': 22, 'B4': 21, 'B5': 20,\n",
    "    'C1': 19, 'C2': 18, 'C3': 17, 'C4': 16, 'C5': 15,\n",
    "    'D1': 14, 'D2': 13, 'D3': 12, 'D4': 11, 'D5': 10,\n",
    "    'E1': 9, 'E2': 8, 'E3': 7, 'E4': 6, 'E5': 5,\n",
    "    'F1': 4, 'F2': 3, 'F3': 2, 'F4': 1, 'F5': 0\n",
    "}\n",
    "\n",
    "# education ordinal mapping\n",
    "education_mapping = {\n",
    "    'Other': -1,\n",
    "    'High School': 0,\n",
    "    \"Bachelor's\": 1,\n",
    "    \"Master's\": 2,\n",
    "    'PhD': 3\n",
    "}\n",
    "\n",
    "# map grade_subgrade and education_level\n",
    "def encode_grade_education(X):\n",
    "    X_copy = X.copy()\n",
    "    X_copy['grade_subgrade'] = X_copy['grade_subgrade'].map(grade_mapping)\n",
    "    X_copy['education_level'] = X_copy['education_level'].map(education_mapping)\n",
    "    return X_copy\n",
    "\n",
    "# categorical columns to encode (excluding education_level now)\n",
    "categorical_features = ['gender', 'marital_status',\n",
    "                        'employment_status', 'loan_purpose']\n",
    "\n",
    "preprocessor = ColumnTransformer(\n",
    "    transformers=[\n",
    "        ('cat', OneHotEncoder(drop='first', sparse_output=False), categorical_features)\n",
    "    ],\n",
    "    remainder='passthrough'  # Keep other columns unchanged (including education_level)\n",
    ")\n",
    "\n",
    "# Create pipeline with grade/education encoding and preprocessor\n",
    "pipeline_edu_ordinal = make_pipeline(\n",
    "    FunctionTransformer(encode_grade_education, validate=False),\n",
    "    preprocessor\n",
    ")"
   ]
  },
